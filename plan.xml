<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<?xml-stylesheet type="text/xsl" href="http://www.eclipse.org/projects/project-plan.xsl"?>
<plan plan-format="1.0" xmlns="http://www.eclipse.org/project/plan" xmlns:html="http://www.w3.org/1999/xhtml" name="UOMo">
  <release projectid="org.eclipse.uomo" version="0.7" />
  <introduction>
    <html:div xmlns="http://www.w3.org/1999/xhtml">
      <p>
        The first goal
        of
        <a href="http://www.eclipse.org/uomo">Eclipse UOMo</a>
        is integration of popular unit standards like UCUM or UnitsML.
      </p>
    </html:div>
  </introduction>
  <release_deliverables>
    <html:div xmlns="http://www.w3.org/1999/xhtml">
      <ul>
        <li>
          <b>UOMo Framework</b>
        </li>
      </ul>
    </html:div>
  </release_deliverables>
  <release_milestones>
    <preamble>
      <html:div xmlns="http://www.w3.org/1999/xhtml">
        <p>
          The plan and roadmap dates are listed on the
          <a href="http://www.eclipse.org/projects/timeline/">Eclipse project timeline</a>
          .
        </p>
      </html:div>
    </preamble>
    <milestone date="2011-01-31" milestone="0.6 RC1">
      <html:div>Release Candidate (API and Feature Complete)</html:div>
    </milestone>
    <milestone date="2011-02-07" milestone="0.6 RC2">
      <html:div>Release Candidate 2</html:div>
    </milestone>
    <milestone date="2011-06-05" milestone="0.6 RC3">
      <html:div>Release Candidate (Final Build)</html:div>
    </milestone>
    <milestone date="2013-06-05" milestone="0.6">
      <html:div>Major Release</html:div>
    </milestone>
    <postamble>
      <html:div xmlns="http://www.w3.org/1999/xhtml">
        <p>
          <b>Tentative Plan</b>
        </p>
        <p>
          <table border="1" align="center">
            <tr>
              <td>
                <b>0.7</b>
              </td>
